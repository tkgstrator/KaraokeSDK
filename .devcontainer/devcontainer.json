{
  "name": "KaraokeSDK",
  "dockerComposeFile": [
    "docker-compose.yaml"
  ],
  "service": "swift",
  "workspaceFolder": "/home/vscode/app",
  "remoteUser": "vscode",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "configureZshAsDefaultShell": true
    },
    "ghcr.io/swift-server-community/swift-devcontainer-features/jemalloc:1": {},
    "ghcr.io/swift-server-community/swift-devcontainer-features/SwiftFormat:0": {},
    "ghcr.io/LosFarmosCTL/swift-devcontainer-features/swiftlint:1": {},
    "ghcr.io/devcontainers/features/github-cli:1": {},
    "ghcr.io/devcontainers/features/git:1": {
      "version": "2.49.0"
    },
    "ghcr.io/dhoeric/features/act:1": {
      "version": "0.2.65"
    },
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {
      "moby": false,
      "dockerDashComposeVersion": "v2"
    }
  },
  "postAttachCommand": "/bin/zsh .devcontainer/postAttachCommand.sh",
  "postCreateCommand": "/bin/zsh .devcontainer/postCreateCommand.sh",
  "postStartCommand": "/bin/zsh .devcontainer/postStartCommand.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "EditorConfig.EditorConfig",
        "GitHub.copilot",
        "GitHub.copilot-chat",
        "PKief.material-icon-theme",
        "antfu.file-nesting",
        "eamodio.gitlens",
        "swiftlang.swift-vscode",
        "vknabel.vscode-swiftformat",
        "vsls-contrib.gitdoc"
      ]
    }
  }
}